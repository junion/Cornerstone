sp {verify-place*propose*place-query
   (state <s> ^name verify-place
              ^superstate.operator <sop>)
   (<sop> ^exec-args <args>)
   (<args> ^place <value>)
-->
   (<s> ^operator <op> +)
   (<op> ^name place-query
         ^actions <act>)
   (<act> ^execute <args>)
   (<args> ^operation place-query)
}
